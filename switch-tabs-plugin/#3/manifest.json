{
	"manifest_version": 3,
	"name": "Switch tabs plugin #3",
	"version": "1.0.0",
	"commands": {
		"open-tab-right": {
			"suggested_key": {
				"default": "Ctrl+Shift+9"
			},
			"description": "Opens tab on the right"
		}
	},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"background": {
		"service_worker": "background.js"
	}
}